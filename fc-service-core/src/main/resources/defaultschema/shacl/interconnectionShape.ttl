@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix gax-validation:  <http://w3id.org/gaia-x/validation#> .

@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix gax-core: <http://w3id.org/gaia-x/core#> .
@prefix gax-node: <http://w3id.org/gaia-x/node#> .
@prefix gax-resource: <http://w3id.org/gaia-x/resource#> .
@prefix gax-participant: <http://w3id.org/gaia-x/participant#> .
@prefix gax-service: <http://w3id.org/gaia-x/service#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix did: <https://www.w3.org/TR/did-core/#> .

gax-validation:InterconnectionShape
	a sh:NodeShape ;
	sh:targetClass gax-resource:Interconnection ;
	sh:property [ sh:path gax-resource:location ;
				  sh:name "location" ;
				  sh:order 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-resource:interface.type ;
				  sh:name "interface.type" ;
				  sh:order 2 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-resource:connectionPointA ;
				  sh:name "connection point a" ;
				  sh:order 3 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-resource:connectionPointZ ;
				  sh:name "connection point z" ;
				  sh:order 4 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-resource:bandwidth ;
				  sh:name "bandwidth" ;
				  sh:order 5 ;
				  sh:node gax-validation:MeasureShape ] ;
	sh:property [ sh:path gax-resource:latency ;
				  sh:name "latency" ;
				  sh:order 6 ;
				  sh:node gax-validation:MeasureShape ] ;
	sh:property [ sh:path gax-resource:availability ;
				  sh:name "availability" ;
				  sh:order 7 ;
				  sh:node gax-validation:MeasureShape ] ;
	sh:property [ sh:path gax-resource:packetLoss ;
				  sh:name "packet loss" ;
				  sh:order 8 ;
				  sh:node gax-validation:MeasureShape ] ;
	sh:property [ sh:path gax-resource:jitter ;
				  sh:name "jitter" ;
				  sh:order 9 ;
				  sh:node gax-validation:MeasureShape ] ;
	sh:property [ sh:path gax-resource:targetPercentile ;
				  sh:name "target percentile" ;
				  sh:order 10 ;
				  sh:node gax-validation:MeasureShape ] ;
	sh:property [ sh:path gax-resource:connectionType ;
				  sh:name "connection type" ;
				  sh:order 11 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-resource:vlanType ;
				  sh:name "vlan type" ;
				  sh:order 12 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-resource:vlanTag ;
				  sh:name "vlan tag" ;
				  sh:order 13 ;
				  sh:node gax-validation:MeasureShape ] ;
	sh:property [ sh:path gax-resource:vlanEtherType ;
				  sh:name "vlan ether type" ;
				  sh:order 14 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-resource:connectedNetwork_A ;
				  sh:name "connected network_ a" ;
				  sh:order 15 ;
				  sh:datatype xsd:decimal ] ;
	sh:property [ sh:path gax-resource:connectedNetwork_Z ;
				  sh:name "connected network_ z" ;
				  sh:order 16 ;
				  sh:datatype xsd:decimal ] ;
	sh:property [ sh:path gax-resource:prefixSet_A ;
				  sh:name "prefix set_ a" ;
				  sh:order 17 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-resource:prefixSet_Z ;
				  sh:name "prefix set_ z" ;
				  sh:order 18 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-service:serviceType ;
				  sh:name "service type" ;
				  sh:order 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-service:publicIpAddressProvisioning ;
				  sh:name "public ip address provisioning" ;
				  sh:order 2 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-service:ipVersion ;
				  sh:name "ip version" ;
				  sh:order 3 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-service:tenantSeparation ;
				  sh:name "tenant separation" ;
				  sh:order 4 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-service:providedBy ;
				  sh:name "provided by" ;
				  sh:order 1 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:nodeKind sh:IRI ] ;
	sh:property [ sh:path gax-service:aggregationOf ;
				  sh:name "aggregation of" ;
				  sh:order 2 ;
				  sh:nodeKind sh:IRI ] ;
	sh:property [ sh:path gax-service:dependsOn ;
				  sh:name "depends on" ;
				  sh:order 3 ;
				  sh:datatype xsd:anyURI ] ;
	sh:property [ sh:path gax-service:termsAndConditions ;
				  sh:name "terms and conditions" ;
				  sh:order 4 ;
				  sh:minCount 1 ;
				  sh:node gax-validation:TermsAndConditionsShape ] ;
	sh:property [ sh:path gax-service:policies ;
				  sh:name "policies" ;
				  sh:order 5 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-service:dataProtectionRegime ;
				  sh:name "data protection regime" ;
				  sh:order 6 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-service:dataExport ;
				  sh:name "data export" ;
				  sh:order 7 ;
				  sh:minCount 1 ;
				  sh:node gax-validation:DataExportShape ] ;
	sh:property [ sh:path gax-service:serviceTitle ;
				  sh:name "service title" ;
				  sh:order 8 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path dct:description ;
				  sh:name "description" ;
				  sh:order 9 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path dcat:keyword ;
				  sh:name "keyword" ;
				  sh:order 10 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-service:provisionType ;
				  sh:name "provision type" ;
				  sh:order 11 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-service:endpoint ;
				  sh:name "endpoint" ;
				  sh:order 12 ;
				  sh:node gax-validation:EndpointShape ] ;
.

gax-validation:TermsAndConditionsShape
	a sh:NodeShape ;
	sh:targetClass gax-core:TermsAndConditions ;
	sh:property [ sh:path gax-core:content ;
				  sh:name "content" ;
				  sh:order 1 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:anyURI ] ;
	sh:property [ sh:path gax-core:hash ;
				  sh:name "hash" ;
				  sh:order 2 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
.

gax-validation:DataExportShape
	a sh:NodeShape ;
	sh:targetClass gax-core:DataExport ;
	sh:property [ sh:path gax-core:requestType ;
				  sh:name "request type" ;
				  sh:order 1 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-core:accessType ;
				  sh:name "access type" ;
				  sh:order 2 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-core:formatType ;
				  sh:name "format type" ;
				  sh:order 3 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
.

gax-validation:EndpointShape
	a sh:NodeShape ;
	sh:targetClass gax-service:Endpoint ;
	sh:property [ sh:path gax-service:endPointURL ;
				  sh:name "end point u r l" ;
				  sh:order 1 ;
				  sh:datatype xsd:anyURI ] ;
	sh:property [ sh:path gax-service:standardConformity ;
				  sh:name "standard conformity" ;
				  sh:order 2 ;
				  sh:nodeKind sh:IRI ] ;
	sh:property [ sh:path gax-service:endpointDescription ;
				  sh:name "endpoint description" ;
				  sh:order 3 ;
				  sh:datatype xsd:anyURI ] ;
.

gax-validation:MeasureShape
	a sh:NodeShape ;
	sh:targetClass gax-core:Measure ;
	sh:property [ sh:path gax-core:value ;
				  sh:name "value" ;
				  sh:order 1 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:float ] ;
	sh:property [ sh:path gax-core:unit ;
				  sh:name "unit" ;
				  sh:order 2 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
.
